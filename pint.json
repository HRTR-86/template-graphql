{
    "rules": {
        "blank_line_after_opening_tag": false,
        "linebreak_after_opening_tag": false,
        "binary_operator_spaces": {
            "operators": {
                "=": "align",
                "=>": "align"
            }
        },
        "phpdoc_align": false,
        "phpdoc_separation": false,
        "no_superfluous_phpdoc_tags": false
    }
}
